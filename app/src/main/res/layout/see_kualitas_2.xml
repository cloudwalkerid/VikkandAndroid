<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:background="@drawable/addnewtamu"
    android:orientation="vertical">
    <TextView
        android:id="@id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/see_view_top"
        android:gravity="center"
        android:fontFamily="@font/calibri"
        android:textSize="20dp"
        android:text="Masuk"/>
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/see_view_bottom"
        android:paddingTop="5dp"
        android:paddingLeft="5dp"
        android:paddingBottom="5dp"
        android:paddingRight="5dp">
        <TextView
            android:id="@+id/barang"
            android:layout_width="match_parent"
            android:textSize="18dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/calibri"
            android:text="barang"/>
        <TextView
            android:id="@+id/kualitas"
            android:layout_width="match_parent"
            android:textSize="18dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/calibri"
            android:text="barang"/>
        <TextView
            android:id="@+id/satuanUnit"
            android:layout_width="match_parent"
            android:textSize="18dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/calibri"
            android:text="barang"/>
        <TextView
            android:id="@+id/nilaiSewa"
            android:layout_width="match_parent"
            android:textSize="18dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/calibri"
            android:text="barang"/>

        <TextView
            android:id="@+id/keterangan"
            android:layout_width="match_parent"
            android:textSize="18dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/calibri"
            android:text="barang"/>
    </LinearLayout>
</LinearLayout>
