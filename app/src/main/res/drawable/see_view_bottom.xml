<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="@color/greyLightCariJadwal"
                />
            <padding
                android:bottom="0dip"
                android:left="0dip"
                android:right="0dip"
                android:top="0dip"
                />
            <corners  android:bottomLeftRadius="6dip" android:bottomRightRadius="6dip"/>
        </shape>
    </item>
    <item android:bottom="4dp" android:top="2dp" android:right="4dp" android:left="4dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
</layer-list>